    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "eslint": "^4.2.0",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.5.0"
